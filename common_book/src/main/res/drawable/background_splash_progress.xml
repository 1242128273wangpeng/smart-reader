<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dimen_radius_10" />
            <stroke android:width="@dimen/dimen_view_width_1" android:color="@color/theme_primary"/>
            <solid android:color="@color/color_white" />
        </shape>
    </item>

    <!-- 第二条进度条颜色 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dimen_radius_10" />
                <gradient
                    android:angle="90.0"
                    android:centerColor="@color/color_green_38dfb8"
                    android:centerY="0.45"
                    android:endColor="@color/color_green_13a784"
                    android:startColor="@color/color_green_1ec39c" />
            </shape>
        </clip>
    </item>
    <!-- 进度条 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dimen_radius_10" />
                <solid android:color="@color/color_green_38dfb8" />
            </shape>
        </clip>
    </item>

</layer-list>
