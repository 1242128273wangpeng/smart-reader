            android:layout_marginLeft="@dimen/dimen_margin_14"
            android:layout_marginRight="@dimen/dimen_margin_14"
            android:layout_marginBottom="@dimen/dimen_margin_19"
            android:background="@drawable/background_feedback_message"
            android:hint="@string/feedback_contact_hint"
            android:textColorHint="@color/color_gray_a6a6a6"
            android:textColor="@color/color_black_2e2e2e"
            android:textSize="@dimen/dimen_text_12"/>
    
    <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_view_height_0"
            android:background="@color/color_white_dadada"/>
