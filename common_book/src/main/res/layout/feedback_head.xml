            android:gravity="center"
            android:background="@color/color_green_e5f0ec"
            android:textSize="@dimen/dimen_text_13"
            android:textColor="@color/color_black_2e2e2e"/>
    
    <TextView
            android:id="@+id/feedback_problem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/feedback_problem"
            android:textColor="@color/color_black_2e2e2e"
            android:textSize="@dimen/dimen_text_15"
            android:layout_marginLeft="@dimen/dimen_margin_14"
            android:layout_marginRight="@dimen/dimen_margin_14"
            android:layout_marginTop="@dimen/dimen_margin_14"
            android:layout_marginBottom="@dimen/dimen_margin_10"/>
    
    <LinearLayout
            android:id="@+id/feedback_problem_first_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/dimen_padding_8"
            android:paddingBottom="@dimen/dimen_padding_8"
            android:layout_marginLeft="@dimen/dimen_margin_14"
            android:layout_marginRight="@dimen/dimen_margin_14"
            android:orientation="horizontal">
        
        <RelativeLayout
                android:id="@+id/feedback_first_problem"
                android:layout_width="@dimen/dimen_view_width_0"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:paddingTop="@dimen/dimen_padding_5"
                android:paddingBottom="@dimen/dimen_padding_5"
                android:background="@drawable/selector_background_white">
            
            <ImageView
                    android:id="@+id/feedback_first_problem_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/feedback_check"/>
            
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/feedback_first_problem_check"
                    android:layout_marginLeft="@dimen/dimen_margin_5"
                    android:text="@string/feedback_first_problem"
                    android:textColor="@color/color_black_2e2e2e"
                    android:textSize="@dimen/dimen_text_14"/>
        
        </RelativeLayout>
        
        <RelativeLayout
                android:id="@+id/feedback_second_problem"
                android:layout_width="@dimen/dimen_view_width_0"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:paddingTop="@dimen/dimen_padding_5"
                android:paddingBottom="@dimen/dimen_padding_5"
                android:background="@drawable/selector_background_white">
            
            <ImageView
                    android:id="@+id/feedback_second_problem_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/feedback_check"/>
            
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/feedback_second_problem_check"
                    android:layout_marginLeft="@dimen/dimen_margin_5"
                    android:text="@string/feedback_second_problem"
