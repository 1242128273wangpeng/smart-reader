<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomPopupDialog">
        <item name="android:windowEnterAnimation">@anim/bottom_dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/bottom_dialog_exit_anim</item>
    </style>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
    </declare-styleable>
</resources>