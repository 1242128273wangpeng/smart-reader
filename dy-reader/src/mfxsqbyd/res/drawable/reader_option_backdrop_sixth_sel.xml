<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="45dp"/>
            <size
                android:height="33dp"
                android:width="33dp"/>
            <solid android:color="@color/read_option_page_bg6"/>
            <stroke android:color="#FFBA01"
                    android:width="1dp"/>
        </shape>
    </item>


    <item android:state_checked="false">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="45dp"/>
            <size
                android:height="33dp"
                android:width="33dp"/>
            <solid android:color="@color/read_option_page_bg6"/>
            <!--<stroke android:color="@color/read_option_page_stroke_bg6"-->
            <!--android:width="1dp"/>-->
        </shape>
    </item>
</selector>